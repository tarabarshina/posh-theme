{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin"
          },
          "leading_diamond": "\r\n \uf105\uf105\uf105",
          "template": " \udb86\udd9f {{ .FormattedMs }}",
          "foreground": "#e4e4e4",
          "type": "executiontime",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "trailing_diamond": "\ue0b0",
          "template": " \ue384 {{ .CurrentDate | date .Format }} \r\n",
          "foreground": "#f4dda4",
          "type": "time",
          "style": "powerline",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "cache_duration": "none",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": " {{ .Icon }} {{ .HostName }} ",
          "foreground": "#49392d",
          "background": "#1ace9e",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }} ",
          "foreground": "#4e4e4e",
          "powerline_symbol": "\ue0b0",
          "background": "#e4e4e4",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\u2026",
            "folder_separator_icon": " <transparent>\ue0b1</> ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#81efd2",
          "powerline_symbol": "\ue0b0",
          "background": "#49392d",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "prefix": " \ue235 "
          },
          "template": " \ue235 (Venv)",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#7154c9",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_gone_icon": "\ueab8",
            "branch_icon": "\uf418 ",
            "branch_identical_icon": "\uf444",
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <#121318>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#121318>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <#121318>\ueb4b {{ .StashCount }}</>{{ end }} ",
          "foreground": "#f2f3f8",
          "background": "#378504",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#a97400{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#54433a{{ end }}",
            "{{ if gt .Ahead 0 }}#744d89{{ end }}",
            "{{ if gt .Behind 0 }}#744d89{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "2006-01-02 Mon MST"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " \ueab0 {{ .CurrentDate | date .Format }} ",
          "foreground": "#81efd2",
          "background": "#49392d",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Root }}#{{else}}${{end}}",
          "foreground": "#1ace9e",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
